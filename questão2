#include <stdio.h>
#include <string.h>

int main()
{
    char string1[30], string2[30];
    printf("Informe uma palavra: ");
    scanf("%s", string1);
    
    printf("Informe outra palavra: ");
    scanf("%s", string2);
    
    printf("\nResultado: ");
    if(strcmp(string1, string2) == 0)
    printf("Iguais");
    else
    printf("Nao sao iguais");

    return 0;
}
